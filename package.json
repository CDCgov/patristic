  },
  "homepage": "https://github.com/CDCgov/patristic#readme",
  "devDependencies": {
    "d3-hierarchy": "^1.1.8",
    "documentation": "^9.1.1",
    "http-server": "^0.11.1",
    "jest": "^24.1.0",
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "terser": "^3.16.1"
  }

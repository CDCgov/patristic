{
  "name": "patristic",
  "version": "0.2.8",
  "description": "Patristic Inference library for Node and Browser",
  "main": "index.js",
  "scripts": {
    "build": "rollup",
    "demo": "http-server",
    "dev": "npm run demo & npm run watch",

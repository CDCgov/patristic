{
  "name": "patristic",
  "version": "0.3.4",
  "description": "Patristic Inference library for Node and Browser",
  "homepage": "https://cdcgov.github.io/patristic/",
  "bugs": "https://github.com/CDCgov/patristic/issues",
  "main": "dist/patristic.js",
  "scripts": {
    "build": "rollup -c",
    "coverage": "./node_modules/jest/bin/jest.js --coverage",
    "demo": "npx serve",
    "dev": "npm run watch & npm run demo",
    "doc": "documentation build src/main.js -f md -o docs/README.md",
    "minify": "terser dist/patristic.js --compress > dist/patristic.min.js",
    "test": "jest",
    "watch": "rollup -c --watch"
  },
  "keywords": [
    "phylogeny"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CDCgov/patristic.git"
  },
  "contributors": [
    "Tony Boyles <nsp3@cdc.gov>"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "documentation": "^9.2.1",
    "jest": "^24.1.0",
    "rollup": "^1.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "terser": "^3.16.1"
  }
}
